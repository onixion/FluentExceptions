<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Authors>Alin Andersen</Authors>
    <Description>Fluent Exceptions library.</Description>
    <Copyright>Copyright Â© 2021 Alin Andersen</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/onixion/FluentExceptions</PackageProjectUrl>
    <RepositoryUrl>https://github.com/onixion/FluentExceptions</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageIcon>Icon.jpg</PackageIcon>
    <PackageTags>fluent, exceptions, exception, fluent-api</PackageTags>
    <AssemblyVersion>1.1.1.0</AssemblyVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Version>1.1.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="TryWrapper.xaml" />
    <None Include="..\..\..\FluentArguments\Source\AlinSpace.FluentArguments\Icon.jpg">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Page Include="TryWrapper.xaml">
      <Generator>MSBuild:Compile</Generator>
    </Page>
  </ItemGroup>

</Project>
